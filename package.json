  "scripts": {
    "__prod": "~~~~~~ PRODUCTION ~~~~~~",
    "r.js": "r.js",
    "build": "r.js -o meta/prod/requirejs-build-config.js paths.requireLib=dist/third-party/requirejs@2 include=requireLib && cp -f index.html 404.html",
    "__dev": "~~~~~~ DEVELOPMENT ~~~~~~",
    "clean": "TODO",
    "copy-third-party": "node meta/scripts/copy-third-party.js && cp node_modules/require-css/css-builder.js dist/third-party && cp node_modules/require-css/normalize.js dist/third-party",
    "dev1": "tsc -p meta/dev --watch",
    "dev": "run-p dev1",
    "start": "browser-sync start --server --port $PORT --logLevel debug --files './*.html,dist/**,content/**' --directory --startPath index-dev.html",
    "start_notes": " --no-open",
    "__misc": "~~~~~~ MISC ~~~~~~",
    "__local_tools": "~~~~~~ Tools installed locally ~~~~~~",
    "browser-sync": "browser-sync",
    "np": "np",
    "nsp": "nsp",
    "tsc": "tsc",
    "__shortcuts": "~~~~~~ Shortcuts ~~~~~~"
  },
  "dependencies": {
    "@offirmo/rx-auto": "^0.2.0",
    "@offirmo/simple-querystring-parser": "^1.1.0",
    "@reactivex/rxjs": "^5.4.0",
    "chroma-js": "^1.3.4",
    "lodash": "^4.17.13",
    "marky": "^1.2.0",
    "packery": "^2.1.1",
    "require-css": "^0.1.10",
    "requirejs": "^2.3.3",
    "tachyons": "^4.7.4",
    "tslib": "^1.7.1",
    "typescript-string-enums": "^0.3.4"
  },
  "devDependencies": {
    "@offirmo/cli-toolbox": "^1.0.0",
    "@types/chroma-js": "^1.1.32",
    "@types/lodash": "^4.14.86",
    "@types/packery": "^1.4.29",
    "@types/randomcolor": "^0.4.3",
    "browser-sync": "^2.18.5",
    "node-typescript-compiler": "^1.1.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "semver": "^5.3.0",
    "typescript": "^2.6.2"
  }
